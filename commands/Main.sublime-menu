[
    {
        "id": "tools",
        "children": [
            {
                "id": "end",
                "caption": "-",
            },
            {
                "caption": "Timesheets: Checkout",
                "command": "timesheets_checkout"
            },
            {
                "caption": "Timesheets: Commit",
                "command": "timesheets_commit"
            },
            {
                "caption": "-",
            }
        ]
    },
    {
        "id": "preferences",
        "children": [
            {
                "caption": "Package Settings",
                "mnemonic": "P",
                "id": "package-settings",
                "children": [
                    {
                        "caption": "Timesheets",
                        "children": [
                            {
                                "caption": "Readme",
                                "command": "open_file",
                                "args": {
                                    "file": "${packages}/timesheets/README.md"
                                }
                            },
                            {
                                "caption": "Changelog",
                                "command": "open_file",
                                "args": {
                                    "file": "${packages}/timesheets/Changelog.md"
                                }
                            },
                            {
                                "caption": "-"
                            },
                            {
                                "caption": "Settings",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/timesheets/settings/timesheets.sublime-settings",
                                    "default": "{\n\t$0\n}\n"
                                }
                            },
                            {
                                "caption": "Key Bindings",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/timesheets/settings/Default (${platform}).sublime-keymap",
                                    "default": "[\n\t$0\n]\n"
                                }
                            },
                            {
                                "caption": "Mouse Bindings",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/timesheets/settings/Default (${platform}).sublime-mousemap",
                                    "default": "[\n\t$0\n]\n"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
]